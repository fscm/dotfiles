# go language
export GOROOT="/usr/local/go"
export GOPATH="${HOME}/Documents/Projects/Go"
export GOBIN="${GOPATH}/bin"

# go binaries and tools
add_to_path "${GOROOT}/bin" after

# go user binaries and tools
add_to_path "${GOBIN}" after

